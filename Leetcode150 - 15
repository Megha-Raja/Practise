Given n non-negative integers representing an elevation map where the width of each bar is 1, compute how much water it can trap after raining.

class Solution:
    def trap(self, height: List[int]) -> int:
        l=0
        r=len(height)-1
        lmax=0
        rmax=0
        res=0
        while l<=r:
            if height[l]<height[r]:
                if lmax<height[l]:
                    lmax=height[l]
                else:
                    res+=lmax-height[l]
                l+=1
            else:
                if rmax<height[r]:
                    rmax=height[r]
                else:
                    res+=rmax-height[r]
                r-=1
        return res
